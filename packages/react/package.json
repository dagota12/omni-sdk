{
  "name": "@omni-analytics/react",
  "version": "0.2.0",
  "description": "React integration for the Omni Analytics SDK. Provides hooks and context providers for seamless analytics tracking in React applications.",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -c rollup.config.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@omni-analytics/sdk": "workspace:*"
  },
  "keywords": [
    "react",
    "analytics",
    "tracking",
    "hooks",
    "context",
    "omni",
    "telemetry"
  ],
  "author": "Dag",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0"
}
